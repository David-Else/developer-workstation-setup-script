{
  "name": "selected-vscode-and-friendly-snippets",
  "engines": {
    "vscode": "^1.11.0"
  },
  "contributes": {
    "snippets": [
      {
        "language": [
          "markdown",
          "rmd"
        ],
        "path": "./snippets/markdown.json"
      },
      {
        "language": [
          "javascript",
          "javascriptreact",
          "vue",
          "svelte"
        ],
        "path": "./snippets/javascript.json"
      },
      {
        "language": [
          "typescript",
          "typescriptreact"
        ],
        "path": "./snippets/typescript.json"
      },
      {
        "language": "php",
        "path": "./snippets/php.json"
      },
      {
        "language": [
          "html"
        ],
        "path": "./snippets/html.json"
      },
      {
        "language": [
          "css",
          "scss",
          "sass",
          "less",
          "stylus"
        ],
        "path": "./snippets/css.json"
      },
      {
        "language": "rust",
        "path": "./snippets/rust.json"
      }
    ]
  }
}
